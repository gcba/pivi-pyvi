[general]
id = ${PIVI_ID}
virtual = ${VIRTUAL}

[logs]
level = Debug
dir = /var/pivi
mail = lucas@lessindustries.com
send_email = True

[serial]
device = /dev/ttyAMA0
baudrate = 115200
timeout = 2.0

[server]
address = ${SERVER_IP}
port = 9000

[GCBA]
server = ${SERVER_API}
token: ${TOKEN}
type_irms = irms
type_vrms = vrms
type_power = power
gcba_id = ${GCBA_ID}
